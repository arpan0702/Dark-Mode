{
    "manifest_version":2,
    "name": "Dark Mode",
    "description": "Turn dark mode on for web pages",
    "version": "1.0.0",
    "icons": {
        "128": "icon128.png"
    },
    "browser_action":{
        "default_icon": "icon19.png",
        "default_title": "Dark Mode",
        "default_popup": "popup.html"
    },
    "permissions":[
        "tabs",
        "https://*/*",
        "http://*/*"
    ]
}